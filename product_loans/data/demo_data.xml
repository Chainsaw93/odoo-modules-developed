<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Almacén de prueba para préstamos -->
        <record id="warehouse_loans_demo" model="stock.warehouse">
            <field name="name">Almacén de Préstamos</field>
            <field name="code">LOANS</field>
            <field name="warehouse_type">loans</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
        
        <!-- Datos de prueba: Contacto para préstamos -->
        <record id="partner_loans_demo" model="res.partner">
            <field name="name">Cliente Préstamo Demo</field>
            <field name="is_company">0</field>
        </record>
    </data>
</odoo>